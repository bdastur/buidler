"BS Stylesheet link": {
 "prefix": "bs-stylesheets",
 "body": [
	 "<!-- bs-stylesheets -->",
	 "<meta charset='utf-8'>"
     "<meta name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no'>",
     "",
     "<link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css' integrity='sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm' crossorigin='anonymous'>",
	 "<script src='https://cdnjs.cloudflare.com/ajax/libs/holder/2.3.2/holder.min.js'",
	 " type='text/javascript'></script>",
     "<link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet'>",
     "<link href='https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css'",
     " rel='stylesheet'",
     " integrity='sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN'",
	 " crossorigin='anonymous'>",
	 "<!-- bs-stylesheets end -->"
 ],
 "description": "Bootstrap style sheets"
},
"BS Javascript sources": {
	"prefix": "bs-src",
	"body": [
		"<!-- bs-src -->",
	    "<script src='https://code.jquery.com/jquery-3.2.1.slim.min.js' integrity='sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN' crossorigin='anonymous'></script>",
        "<script src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js' integrity='sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q' crossorigin='anonymous'></script>",
		"<script src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js' integrity='sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl' crossorigin='anonymous'></script>",
		"<!-- bs-src end -->",
	],
	"description": "Bootstrap javascript sources"
}

"HTML Comment": {
	"prefix": "hcomment",
	"body": [
		"<!-- hcomment -->",
		"<!-- $1","-->"
	],
	"description": "Html comment"
},
"BS Alert": {
	"prefix": "bs-alert",
	"body": [
		"<!-- bs-alert -->",
		"<div class='alert alert-${1:primary|secondary|success|danger|warning|info|light|dark}' role='alert'>",
		"    ${2:Alert message!}",
		"</div>",
		"<!-- bs-alert end -->",
	],
	"description": "Bootstrap Alert"
},
"BS Dismissable Alert": {
	"prefix": "bs-alert-dismissible",
	"body": [
		"<!-- bs-alert-dismissible -->",
		"<div class='alert alert-${1:primary|secondary|success|danger|warning|info|light|dark} alert-dismissible fade show' role='alert'>",
		"    ${2:Alert message!}",
		"    <button type='button' class='close' data-dismiss='alert' ari-label='Close'>",
		"        <span aria-hidden='true'>&times;</span",
		"    </button>",
		"</div>",
		"<!-- bs-alert-dismissible end -->",
	],
	"description": "Bootstrap Alert"
},
"BS Alert Link": {
	"prefix": "bs-alert-link",
	"body": [
		"<!-- bs-alert-link -->",
		"<a href='${1:#}' class='alert-link'>${2:Example link}</a>",
		"<!--  bs-alert-link end-->",
	],
	"description": "Bootstrap Alert Link"
},
"BS Alert Heading": {
	"prefix": "bs-alert-heading",
	"body": [
		"<!--  bs-alert-heading-->",
		"<h${1:4} class='alert-heading'>${2:Alert Heading Message}</h>",
		"<!-- bs-alert-heading end -->",
	],
	"description": "Bootstrap Alert Heading"
},
"BS Badge": {
	"prefix": "bs-badge",
	"body": [
		"<!-- bs-badge -->",
		"<span class='badge badge-${1:primary|secondary|success|danger|warning|info|light|dark} ${2:badge-pill}'>${3:Badge text}</span>",
		"<!-- bs-badge end -->",
	],
	"description": "Bootstrap Badge"
},
"BS Badge Link": {
	"prefix": "bs-badge-link",
	"body": [
		"<!-- bs-badge-link -->",
		"<a href='${1:#}' class='badge badge-${2:primary|secondary|success|danger|warning|info|light|dark} ${3:badge-pill}'>${4:Badge text}</a>",
		"<!-- bs-badge-link end -->",
	],
	"description": "Bootstrap Badge link"
},
"BS Button": {
	"prefix": "bs-button",
	"body": [
		"<!-- bs-button -->",
		"<button type='button' class='btn btn-${1:outline-}${2:primary|secondary|success|danger|warning|info|light|dark}",
		" ${3:btn-lg} ${4:disabled} ${5:btn-block}'>${6:submit}</button>",
		"<!-- bs-button end -->",
	],
	"description": "Bootstrap Button"
},
"BS Card": {
	"prefix": "bs-card",
	"body": [
		"<!-- bs-card -->",
		"<div class='card ${1:text-center|text-right}' style='width: ${2:18rem}'>",
		"    ${3:<div class='card-header'>Header</div>}",
		"    ${4:<img class='card-img-top' src='...'' alt='Card image cap'>}",
		"    <div class='card-body'>",
		"        ${5:<h5 class='card-title'>Card title</h5>}",
		"        ${6:<h6 class='card-subtitle mb-2 text-muted'>Card subtitle</h6>}",
		"        ${7:card-content}",
		"        ${8:<a href='#'' class='card-link'>Card link</a>}",
		"        ${9:<div class='card-footer text-muted'>Footer</div>}",
		"    </div>",
		"</div>",
		"<!-- bs-card end -->",
	],
	"description": "Bootstrap card"
},
"BS Card Listgroup": {
	"prefix": "bs-card-listgroup",
	"body": [
		"<!-- bs-card-listgroup -->",
		"<div class='card ${1:text-center|text-right}' style='width: ${2:18rem}'>",
		"    ${3:<div class='card-header'>Header</div>}",
		"    ${4:<img class='card-img-top' src='...'' alt='Card image cap'>}",
		"    <div class='card-body'>",
		"        ${5:<h5 class='card-title'>Card title</h5>}",
		"        ${6:<h6 class='card-subtitle mb-2 text-muted'>Card subtitle</h6>}",
		"        ${7:card-content}",
		"        ${8:<a href='#'' class='card-link'>Card link</a>}",
		"    </div>",
		"    <ul class='list-group list-group-flush'>",
		"       <li class='list-group-item'>List Item</li>",
		"    </ul>",
		"    <div class='card-body'>",
		"        ${9:<div class='card-footer text-muted'>Footer</div>}",
		"    </div>",
		"</div>",
		"<!-- bs-card-listgroup end -->",
	],
	"description": "Bootstrap card listgroup"
},
