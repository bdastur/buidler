		"        </button>",
		"      </h5>",
		"    </div>",
		"    <div id='${3:card-body-id}' class='collapse' ",
		"     aria-labelledby='${1:heading-id}' data-parent='#${4:accordion-id}'>",
		"      <div class='card-body'>",
		"         ${6:card body content}",
		"      </div>",
		"    </div>",
		"  </div>",
		"<!-- bs-accordian-card end -->",
	],
	"description": "Bootstrap accordian card"
},

"BS accordian tablist": {
	"prefix": "bs-accordian-tablist",
	"body": [
		"<!-- bs-accordian-tablist -->",
		"<div id='accordian' role='tablist'>",
		"    <div class='card'>",
		"        <div class='card-header' role='tab' id='${1:headingOne}'>",
		"            <h5 class='mb-0'>",
		"                <a data-toggle='collapse' href='#${2:one}'",
		"                 role='button' aria-expanded='true' aria-controls='${2:one}'>",
		"                ${3:Group Item heading}",
		"                </a>",
		"            </h5>",
		"        </div>",
		"        <div id='${2:one}' class='collapse show' role='tabpanel'",
		"         aria-labelledby='${1:headingOne}' data-parent='#accordian'>",
		"            <div class='card-body'>",
		"                card body details!",
		"            </div>",
		"        </div>",
		"    </div>",
		"</div>",
		"<!-- bs-accordian-tablist end -->",

	],
	"description": "Bootstrap collapse - tablist"
},
